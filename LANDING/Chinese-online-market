	if($('#keywords').val()!=""&&$('#keywords').val()!=$('#keywords').attr('dvalue')){

		jumpToUrl($('#quick_find_header').attr('action')+'&prm='+litb.prm['site']+'.'+litb.prm['page']+'.62.0&keyword='+$('#keywords').val());
	}
});

