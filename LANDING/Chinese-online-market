<script type="text/javascript">
$(function() {
	$('#loginMobile').keyup(function(){
		this.value = this.value.replace(/[^\d]/g, '');
	});
	$('#area_code').chosen({width: 88});
    if ($('.recaptcha-td').length) {
        if($('.mobilenumber-box').is(":visible")) {
            $('.mobilenumber-box').append($('#recaptchaTr'));
        } else {
            $('.email-box').append($('#recaptchaTr'));
        }
    }
	$('#eorp').click(function(){
		if($('.mobilenumber-box').is(":visible")){
				$(this).html($(this).attr('data-default'));
				$('.mobilenumber-box').hide();
				$('.mobilenumber-box input').attr('disabled',true);
				$('.email-box input').attr('disabled',false);
				$('.email-box').show();
				if ($('.recaptcha-td').length) {
                    $('.email-box').append($('#recaptchaTr'));
                }
	}else{
				$(this).html($(this).attr('data-text'));
				$('.email-box input').attr('disabled',true);
				$('.mobilenumber-box input').attr('disabled',false);
				$('.email-box').hide();
				$('.mobilenumber-box').show();
                if ($('.recaptcha-td').length) {
                    $('.mobilenumber-box').append($('#recaptchaTr'));
                }
	}
	});
	
	var items = {
		login_email_address : [{type:'null', errMsg:litb.langs.ckReg.noEmailAddress}, {type:'email', errMsg:litb.langs.ckReg.signinInvalidEmail}],	
		login_password : [{type:'null', errMsg:litb.langs.ckReg.noPassword}, {type:'minlength', minlength:5, errMsg:litb.langs.ckReg.invalidPassword.stringFormat('5')}]
	};
	var mobileitems={
				login_mobile_number : [{type:'null', errMsg: "Please enter a mobile number. "},{type:'minlength', minlength:5, errMsg: "Please enter at least 5 characters. "}],
				login_mobile_password : [{type:'null', errMsg:litb.langs.ckReg.noPassword},{type:'minlength', minlength:5, errMsg:litb.langs.ckReg.invalidPassword.stringFormat('5')}]
			};
					$('#loginForm .recaptcha-error-message').hide();
	    //facebook register email
    $("#popfbCancel").click(function(e){
        e.preventDefault();
        trackingClickDom($('#popfbCancel'));
        $(".pop-facebook-html").hide();
        $("#lightboxOverlay").hide();
        $(".error").show();
    });

	$("#fbregisterEmail").autocompleteEmail();

	var fbitems=[{
				register_email_address : [{
						type:'null',
						errMsg:litb.langs.ckReg.fbinvalidEmailAddress
					},
						{
							type:'email',
							errMsg:litb.langs.ckReg.fbinvalidEmailAddress
						}
			]}];
	$('#popfbSubmit').click(function(e){
		var form = '#newfbAccountForm';
		$('.control-group', form).removeClass('error').children('.help-inline, .help-block').text('');
		$('.control-group', form).children('.help-block').addClass('help-block-display');
		var result = $(form).formCheck(fbitems[0],
			{showError: function(obj, errMsg) {
					if (obj.parent().hasClass('control-group')) {
						obj.parent().addClass('error').children('.help-inline').text(errMsg);
					} else {
						obj.wrap('<div class="control-group error"></div>').after(' <span class="help-inline">' + errMsg + '</span>');
					}
				$('#popfbSubmit').blur();
				obj.focus(clearOnFocus);
			}}
		);
		if(result == false){
			return false;
		}
		$.ajax({
			type: 'POST',
			url: litb.baseURL + 'index.php?main_page=info_check&action=json_checkEmail',
			data: 'email='+$('#fbregisterEmail').val(),
			async: false,
			success: function(data) {
				data = eval('[' + data + ']')[0];
				if (data.success) {
					$('#fbregisterEmail').parent('.control-group').removeClass('error').children('.help-inline').text('');
				} else {
					result=false;
					if ($('#fbregisterEmail').parent().hasClass('control-group')) {
						$('#fbregisterEmail').parent().addClass('error').children('.help-inline').text(litb.langs.ckReg.fbexistEmailAddress);
					} else {
						$('#fbregisterEmail').wrap('<div class="control-group error"></div>').after(' <span class="help-inline">'+litb.langs.ckReg.fbexistEmailAddress+'</span>');
					}
				}
			}
		});
		if(result == false){
			return false;
		}else{
			$("#popfbSubmit").attr('disabled', true).text(litb.langs.processing+ "...");
			trackingClickDom($('#popfbSubmit'));
			$("#newfbAccountForm").submit();
		}
    });

	$('#loginForm').submit(function() {
	    //report GA
        		var status;
		if($('.mobilenumber-box').is(":visible")){
			$('.mobilenumber-box .errorInfo').html('');
			status = $(this).formCheck(mobileitems, {errinfoFinder: function(obj) {return obj.next();}});
		}else{
			$('.email-box .errorInfo').html('');
			status = $(this).formCheck(items, {errinfoFinder: function(obj) {return obj.next();}});
		}
		if(status){
			if($('#loginForm .recaptcha-td').length > 0 && $('#loginForm .grecaptcha-id').val() == ''){
				$('#loginForm .recaptcha-error-message').show();
				return false;
			}
			$('.signInBtn').attr('disabled',true);
			return true;
		}else{
			return false;
		}
	});

	$('#loginEmail').autocompleteEmail();

	// facebook-login click
	$(".fb-button a").click(function(){
        //report GA
        		var $this = $(this);
		$this.addClass("sign-with-faceBook").find(".fb-ctnt").text(litb.langs.processing + "...");
	});
	$(".gl-button a").click(function(){
        //report GA
        		var $this = $(this);
		$this.addClass("sign-with-google").find(".gl-ctnt").text(litb.langs.processing + "...");
	});
    $('.success').append( '<i class="iconfont iconwenhao" style="font-size:13px;">&#xe787;</i>');
        $('.iconwenhao').css('margin-left','5px');
        var $questionIcon = $('.iconwenhao');
    var $popUpWindow = $('.newPwdPopUp');
    function initDrainage() {
        var hideTimer = null;
        if (!$popUpWindow.length) return null;
        function showWrapper() {
            clearTimeout(hideTimer);
            $popUpWindow.addClass('show');
        }
        function hideWrapper() {
            clearTimeout(hideTimer);
            hideTimer = setTimeout(function() {
                $popUpWindow.removeClass('show');
            }, 100);
        }
        $questionIcon.on('mouseenter', function() {
            showWrapper();
        });
        $questionIcon.on('mouseleave', function() {
            hideWrapper();
        });
        $popUpWindow.on('mouseenter', function() {
            showWrapper($(this));
        });
        $popUpWindow.on('mouseleave', function() {
            hideWrapper($(this));
        });
    }
    $(window).on('scroll', function() {
        var $popUpWindow = $('.newPwdPopUp');
        $popUpWindow.removeClass('show');
    });
    initDrainage();
    $('.customer-service').attr('href', 'https://gw.lightinthebox.com/fr/index.php?main_page=submit_account_issue_ticket&from=login');
});
function clearOnFocus() {
	$(this).parent('.control-group').removeClass('error').children('.help-inline').text('');
}
</script><!-- widget-sign-in_eof -->
