<script type="text/javascript">
		$('#newAccountForm .recaptcha-error-message').hide();
	
$('#country').chosen({width:220});

$(function() {
    var items = {
    	register_email_address : [{type:'null', errMsg:litb.langs.ckReg.noEmailAddress}, {type:'email', errMsg:litb.langs.ckReg.invalidEmailAddress}],
    	register_confirm_email_address : [{type:'null', errMsg:litb.langs.ckReg.noConfirmEmailAddress}, {type:'confirmEmail', errMsg:litb.langs.ckReg.emailAddressesNotMatch}],	
    	register_password : [{type:'null', errMsg:litb.langs.ckReg.noPassword}, {type:'patternCheck', errMsg:litb.langs.ckReg.registerInvalidPassword.stringFormat('7')}],
    	register_confirmation : [{type:'null', errMsg:litb.langs.ckReg.noConfirmPassword}, {type:'matchPassword', errMsg:litb.langs.ckReg.passwordNotMatch}],
    	register_country_id : [{type:'select', value:-1, errMsg:litb.langs.ckReg.noCountry}],
    	register_privacy_conditions : [{type:'checked', value:-1, errMsg:litb.langs.ckReg.notAgreeTerms}]
    };

	$('#newAccountForm').submit(function() {
		$(this).siblings('.error').hide();
		$('.errorInfo', this).html('');
		$('#country_chzn').hide().show();
		var status = $(this).formCheck(items,
			{errinfoFinder: function(obj) {return obj.siblings(':last');},
			rules: {
				patternCheck:function(obj, checks){
					if($(obj).val() != '' && $(obj).val().match(/\d/) && $(obj).val().match(/[a-zA-Z]/) && $(obj).val().length >= 7){
						$('.errorInfo.password').hide();
						return true;
					} else {
						$('.lightGray').hide();
						$('.errorInfo.password').show();
						return false;
					}
				},
				matchPassword : function(obj, checks) {return $(obj).val() == $('#registerPassword').val();},
				confirmEmail : function(obj, checks) {return $(obj).val().toLowerCase() == $('#registerEmail').val().toLowerCase();}}});
		if($('#newAccountForm .g-recaptcha')[0] && register_grecaptchaId.val()==''){
			status = false;
			$('.recaptcha-error-message').show();
		}
		if(status){
			$('.signUpBtn').attr('disabled',true);
			return true;
		}else{
			return false;
		}
	});

	//失焦时判断格式
	$('#registerEmail').bind("blur", function() {
		var _itme = {
			register_email_address : [
				{type:'null', errMsg:litb.langs.ckReg.noEmailAddress},
				{type:'email', errMsg:litb.langs.ckReg.invalidEmailAddress}
			]
		};
		formCheckBlur(_itme);
	});

	$('#registerConfirmEmail').bind("blur", function() {
		var _itme = {
			register_confirm_email_address : [
				{type:'null', errMsg:litb.langs.ckReg.noConfirmEmailAddress}, 
				{type:'confirmEmail', errMsg:litb.langs.ckReg.emailAddressesNotMatch}
			]
		};
		formCheckBlur(_itme);
	});

	$('#registerPassword').bind("blur", function() {
		var _itme = {
			register_password : [
				{type:'null', errMsg:litb.langs.ckReg.noPassword},
				{type:'patternCheck', errMsg:litb.langs.ckReg.registerInvalidPassword.stringFormat('7')}
			]
		};
		formCheckBlur(_itme);
	});

	$('#registerConfirmation').bind("blur", function() {
		var _itme = {
			register_confirmation : [
				{type:'null', errMsg:litb.langs.ckReg.noConfirmPassword}, 
				{type:'matchPassword', errMsg:litb.langs.ckReg.passwordNotMatch}
			]
		};
		formCheckBlur(_itme);
	});

	function formCheckBlur(itme){
		$('#newAccountForm').formCheck(itme,
			{errinfoFinder: function(obj) {return obj.siblings(':last');},
    		rules: {
				patternCheck:function(obj, checks){
					if($(obj).val() != '' && $(obj).val().match(/\d/) && $(obj).val().match(/[a-zA-Z]/) && $(obj).val().length >= 7){
						$('.errorInfo.password').hide();
						return true;
					} else {
						$('.lightGray').hide();
						$('.errorInfo.password').show();
						return false;
					}
				},
				matchPassword : function(obj, checks) {return $(obj).val() == $('#registerPassword').val();},
    			confirmEmail : function(obj, checks) {return $(obj).val().toLowerCase() == $('#registerEmail').val().toLowerCase();}}}
		);
	}

//    $('#registerEmail').change(function() {
//        $('#newAccountForm').formCheck({register_email_address : [{type:'null', showError:function(){}, noFocus:true}, {type:'email', showError:function(){}, noFocus:true}, {type:'availEmail', noFocus:true}]},
//        	{errinfoFinder: function(obj) {return obj.siblings(':last');},
//        	rules: {availEmail : function(obj, checks) {
//        		$('#regEmailValWait').show();
//        		$.ajax({
//        			type: 'POST',
//        			url: litb.baseURL + 'index.php?main_page=info_check&action=json_checkEmail&language=//',
//        			data: 'email='+$(obj).val()+'//',
//        			complete : function() {$('#regEmailValWait').hide();},
//        			success: function(data) {
//        				data = eval('[' + data + ']')[0];
//        				if (data.success) {
//        					$(obj).siblings(':last').text('');
//        				} else {
//        					$(obj).siblings(':last').text(litb.langs.ckReg.existEmailAddress);
//        				}
//        			}
//        		});
//        	}
//        	}}
//        );
//    });
    
	$('#registerPassword').focus(function(){
		$('.lightGray').show();
	});

    $('#newfbAccountForm button[type="submit"]').click(function(){
                $(this).attr('disabled', true).text(litb.langs.processing+ "...");
        $(this).submit();
    });
});

$("#registerEmail").autocompleteEmail();
$("#registerConfirmEmail").autocompleteEmail();


</script><!-- widget-sign-up_eof --></div>
</div>
<script>
$.fn.prmListener = function(options){
	var el = $(this);
	if(!litb.prm["page"]||el.attr('href').indexOf('prm')!=-1||!el.parents('.prm').attr('data-prm')) return;
	var options = $.extend({
			'prm' : litb.prm["site"] + '.' + litb.prm["page"]	+ '.' + el.parents('.prm').attr('data-prm') +  '.'+options.index
		},
		options
	);
	if(el.attr('href').indexOf('?')!=-1){
		el.attr('href',(el.attr('href')+'&prm='+options.prm));
	}else{
		el.attr('href',(el.attr('href')+'?prm='+options.prm));
	}
};
$('.prm').each(function(n){
	$(this).find('a').each(function(n){
		if($(this).parents('.prm').find('a').length == 1){
			$(this).attr({'data-index':n});
		}else{
			$(this).attr({'data-index':++n});
		}
				$(this).mousedown(function() {
				$(this).prmListener({'index':$(this).attr('data-index')});
			});
		});
});
</script></div>

<link href="https://lijc.rightinthebox.com/resource_9b24c72c58f2c7ef864de884b58649b925967978_litb//dev_v2/css/new_footer_2020/new_footer_2020.css" rel="stylesheet"/><script>
function initFooter (){
	/*payal以及paypal-verified点击链接打开新窗口先关操作*/
	$('.paypal').click(function() {
		window.open('https://www.paypal.com/us/cgi-bin/webscr?cmd=xpt/Marketing/popup/OLCWhatIsPayPal-outside', 'olcwhatispaypal', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=yes, resizable=yes, width=400, height=350');
		return false;
	});
	$('.paypal-verified').click(function() {
		window.open('https://www.paypal.com/verified/pal=order@litb-inc.com', 'olcwhatispaypal', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=yes, resizable=yes, width=400, height=350');
		return false;
	});
	var box = $('.our-newsletters');
	$('.footer-sub-addr').focus(function() {
		var me = $(this);
		if (me.val() == me.attr('data-value')) {
			me.val('').addClass('valid');
            if ($('.pagelet-footer').hasClass('new-pagelet-footer')) {
                me.autocompleteEmail({
                    width:12,
                    resultsClass: 'newResultsEmail'
                });
                $('#resultsEmail').addClass('newResultsEmail');
            } else {
                me.autocompleteEmail({width:8});
            }
		}
	}).blur(function() {
		var me = $(this);
		if (me.val() == '') {
			me.val(me.attr('data-value')).removeClass('valid');
		}
	});
    if ($('.pagelet-footer').hasClass('new-pagelet-footer')) {
        $('.footer-sub-addr').autocompleteEmail({
            resultsClass: 'mailInstant newResultsEmail',
            width:12,
        });
        $('#resultsEmail').addClass('newResultsEmail');
        if (litb.website === 'miniinthebox') {
            $('.new-pagelet-footer').addClass('mini');
        }
    } else {
        $('.footer-sub-addr').autocompleteEmail();
    }
    box.find('.error-info-email').css({
        width: box.find('form').width()
    });
	box.find('form').submit(function(){
        if (!/^[\w.-]+\@[a-zA-Z0-9.-]+\.[a-zA-Z]{2,4}$/.test($('.footer-sub-addr').val())) {
            $('.footer-sub-addr').focus();
            if (box.parents('.pagelet-footer').hasClass('new-pagelet-footer')) {
                box.find('.error-info-email').text(box.data('check'));
                box.find('.error-info-email').show();
            } else{
                alert(box.data('check'));
            }
            return false;
        } else {
            $('.footer-sub-addr').focus();
            if (box.parents('.pagelet-footer').hasClass('new-pagelet-footer')) {
                box.find('.error-info-email').text('');
                box.find('.error-info-email').hide();
            }
        }
	})
}
</script><!--// featureAB判断是否添加 new-pagelet-footer类名-->
<footer class="pagelet pagelet-footer new-pagelet-footer  "><script>
			$.ajax({
				url: litb.baseURL + 'index.php?main_page=info_check&action=ajax_get_tpl_footer&language=fr',
				type: 'GET',
				data:'',
				success: function(data) {
					$('.pagelet-footer').prepend(data);
					initFooter();
				}
			});
		</script></footer>

