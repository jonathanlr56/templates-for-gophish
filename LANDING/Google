    function generateUUID() {
    var dt = new Date().getTime();
    var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
        var r = (dt + Math.random()*16)%16 | 0;
        dt = Math.floor(dt/16);
        return (c=='x' ? r :(r&0x3|0x8)).toString(16);
    });
    return uuid;
}

function getCookieDclic(nomCookie) {
  deb=document.cookie.indexOf(nomCookie+"=");
  if (deb>=0) {
    deb+=nomCookie.length+1;
    fin=document.cookie.indexOf(";",deb);
    if (fin<0) {
      fin=document.cookie.length;
    }
    return unescape(document.cookie.substring(deb,fin));
  }
  else {
     return "";
  }
}

function setLocalCookieDclic(value) {
  if (!value) {
    value =  generateUUID();
  }
  var date = new Date();
  date.setTime(date.getTime()+(13*30*24*60*60*1000));
  document.cookie = "dclic_tid_local="+ value +"; expires="+date.toUTCString()+"; path=/";
}

if (typeof(navigator) != "undefined" && navigator.doNotTrack != "1" && navigator.msDoNotTrack != "1" && typeof(window) != "undefined" && window.doNotTrack != "1") {
if (typeof(getCookieDclic) != "undefined" && getCookieDclic('cookies_accepted') != null && getCookieDclic('cookies_accepted') != "") {
  if (getCookieDclic('cookies_accepted') == "1" || getCookieDclic('cookies_accepted').match("cookie_marketing=true")) {

    function getOrigin() {
      var subsites = ["cmne", "cmmabn"];
      var urlpart = window.location.pathname.split('/')[1];

      var found = false;
      for (var i = 0; i < subsites.length; i++) {
        if (subsites[i] == urlpart) {
          found = true;
          break;
        }
      }

      return found === true ? window.location.origin + "/" + urlpart : window.location.origin;

      //return subsites.includes(urlpart) == true ? window.location.origin + "/" + urlpart : window.location.origin;
    }

    var dclicClient = {
      info: {
        v: "1.0.0"
      },
      watMap: {
        "espace": "s1",
        "user_federation": "s2",
        "user_agence": "s3",
        "user_formule_bad": "s4",
        "user_type_client": "s5",
        "user_segment": "s8",
        "user_segment_commercial":"s9",
        "user_frequence_bad": "s10",
        "env_work": "s11",
        "env_langue": "s12",
        "dir_reg": "s13",
        "cat_juridique": "s14",
        "env_espace": "s15",
        "typepage": "s16",
        "user_space": "s17",
        "statutuser": "s18",
        "substitution_mode": "s19",
        "user_type_personne": "s20",
        "user_niveau": "s21",
        "env_channel": "s22",
        "relation_group": "s23",
        "env_marque": "s24",
        "env_country": "s25",
        "page_cat1": "s26",
        "page_cat2": "s27",
        "page_cat3": "s28",
        "page_cat4": "s29",
        "page_cat5": "s30"
      },

      init: function() {
        if (typeof(setLocalCookieDclic) != "undefined" && (getCookieDclic('dclic_tid_local') == null || getCookieDclic('dclic_tid_local') == "" )) {
          setLocalCookieDclic()
        }

        document.addEventListener("DOMContentLoaded", function() {
          dclicClient.pushEvent("Page Viewed");
        });
      },
      setEid: function(value) {
        dclicClient.info.eid = value;
      },
      setAid: function(value) {
        dclicClient.info.aid = value;
      },
      pushEvent: function(name, payload) {
        var evt = {};
        payload = payload || {};

        $.extend(evt, payload);

        if(typeof watData !== 'undefined' && typeof tc_vars !== 'undefined') {
            Object.assign(watData, tc_vars);
        }

        Object.keys(dclicClient.watMap).forEach(function (key, index) {
               if(typeof watData !== 'undefined') {
                   if(watData.hasOwnProperty(key)) {
                        evt[dclicClient.watMap[key]] = watData[key];
                   }
               }
         });

        $.extend(evt, dclicClient.info);
        $.extend(evt, {
          e: name,
          oc: new Date().toISOString(),
          dl: document.documentElement.lang,
          ss: window.screen.width + "x" + window.screen.height,
          sc: window.screen.colorDepth,
          tz: new Date().getTimezoneOffset(),
          bs: window.innerWidth + "x" + window.innerHeight,
          pc: window.location.pathname.substring(0, 255),
          ap: ( typeof url_page !== "undefined" ) ? url_page : "",
          ps: (document.referrer.length > 0 && document.referrer.lastIndexOf(window.location.origin, 0) === 0
                  ? (function parseurl(){var parser = document.createElement('a'); parser.href = document.referrer; return parser.pathname.substring(0, 255);})()
                  : ""),
          pt: document.title,
          px: Math.round(window.pageXOffset),
          py: Math.round(window.pageYOffset),
          s6: ( typeof clientreconnu !== "undefined" && clientreconnu === "Y" ? true : false ),
          s7: (typeof alias !== "undefined" ? alias : ""),
          s11: (window.location.href.split('/')[2].charAt(3) === "-") ? window.location.href.split("/")[2].split("-")[0] : "prod",
          origin: getOrigin()
        });

        $.ajax({
            url: "https://dclic-events.e-i.com/",
            type: "POST",
            headers: { "Access-Control-Allow-Origin": "*" },
            crossDomain: true,
            contentType: "application/json",
            data: JSON.stringify({dclic_tid_local: getCookieDclic("dclic_tid_local"), data: evt}),
            dataType: "json",
            xhrFields: { withCredentials: true },
            success: function (res) {
              var tid = null;
              if (res) {
                tid = res.dclic_tid;
                setLocalCookieDclic(res.dclic_tid);
              }
              setTimeout(function() {dclicClient.resolve(tid)}, 5000);
    	      }
        });
      },
      resolve: function(dclic_tid) {
        var url = "https://dclic-resolver.e-i.com/";
        if (dclic_tid) {
          url += dclic_tid;
        }
        $.ajax({
          url: url,
          type: "GET",
          headers: { "Access-Control-Allow-Origin": "*" },
          crossDomain: true,
          contentType: "application/json",
          data: {
            origin: getOrigin()
          },
          dataType: "json",
          xhrFields: { withCredentials: true },
          success: function(res, status) {
            if (res !== null && 'dclicseg' in res) {
              document.cookie = "dclic_seg=" + res.dclicseg + "; path=/";
            }
          }
        });
      }
    };

    dclicClient.init();

    
    function eia(action, data){
      if (action === "set") {
        dclicClient.setEid(data.eid);
        dclicClient.setAid(data.aid);
      }
    }
  }
}
}
    
